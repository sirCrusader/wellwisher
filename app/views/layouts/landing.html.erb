<!DOCTYPE html>
<html>
<head>
  <title>Wellwisher</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body background="<%= asset_path '2.jpg' %>">
<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <%= link_to 'Well Wisher', unauthenticated_root_path, class: 'navbar-brand' %>
    </div>
    <div>
      <ul class="nav navbar-nav">
        <li><a href="#">About</a></li>
      </ul>
      </li>
      <ul class="nav navbar-nav">
        <li><a href="#">Contacts</a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
            <li><%= link_to 'Sign in', new_user_session_path %> or <%= link_to 'Sign up', new_user_registration_path %></li>
      </ul>

    </div>
  </div>
</nav>

<div class="container row">
  <%= yield %>
</div>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
</body>
</html>
